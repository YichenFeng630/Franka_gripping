<launch>
  <!-- Grasp Planning Pipeline V3 (Enhanced Robustness) -->
  <!-- Features: Multi-directional approach, layered retry, retreat stage, internal normalization -->

  <rosparam command="load" file="$(find panda_grasp_planning)/config/grasp_params.yaml" />
  <rosparam command="load" file="$(find panda_grasp_planning)/config/planning_params.yaml" />

  <!-- V3 Pipeline: Multi-directional + Layered Retry + Retreat -->
  <node 
    name="grasp_pipeline_node_v3" 
    pkg="panda_grasp_planning" 
    type="grasp_pipeline_v3.py" 
    output="screen"
    required="true">
    <param name="use_gripper" value="false" />
  </node>

</launch>
